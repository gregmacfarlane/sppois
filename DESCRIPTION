Package: sppois
Type: Package
Title: Spatial Autoregressive Poisson Modeling
Version: 0.0.0.9001
Authors@R: person("Greg", "Macfarlane", email = "gregmacfarlane@gmail.com",
  role = c("aut", "cre"))
